                <span v-show="pageIndex>1 & pageIndex< data.length - 1">{{pageIndex - 1}}/{{data.length-3}}</span>
            </div>
        </div>
    </div>
</nav>

<div class="container-fluid">
    <div class="row">
        <div class="col-xs-12" id="content">
            <div v-for="(item,index) in data">
                <div style="position:relative" v-show="item.show" v-html="item.html" v-bind:show="item.show"></div>
            </div>
            <div class="margin-top-50">
                <button type="button" v-show="showPrev" class="btn btn-primary" @@click="prev">{{btPrev}}</button>
                <button type="button" class="btn btn-primary  pull-right" @@click="next">{{btNext}}</button>
            </div>
        </div>
    </div>
</div>
