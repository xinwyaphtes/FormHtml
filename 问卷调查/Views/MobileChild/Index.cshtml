                    //key: $('#hd_mainId').val(),
                    childInfo: childInfo,
                    data: JSON.stringify(tableData),
                    //dataResult: dataResult
                },
                function (data) {
                    $('#content').html("<h3>您已完成全部问卷，谢谢合作！</h3>");
                });

