                    break;
                case 2:
                    break;
                    //选项多选一,选中后，有后续内容必填

                    var objs = [{ group: '#A1_1,#A1_2,#A1_4', aftersel: ['#A1_2|#A1_3', '#A1_4|#A1_5,#A1_6'] }, { group: '#A1_7,#A1_8', aftersel: ['#A1_8|#A1_9'] }];

                    var o = needcheckckbox(objs);

                    if (o) {
