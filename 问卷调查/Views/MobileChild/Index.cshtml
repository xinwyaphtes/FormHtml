        function save() {
            var tableData = [];
            var childInfo = {
                Name: $('input[name="name"]').val(),
                Type: 1,
                Company: $('input[name="school"]').val(),
                Department: $('input[name="class"]').val(),
                EmployeeNo: $('input[name="stdno"]').val(),
            }

            $.each($('[parent]'),
                function () {
                    var pIndex = $(this).attr('parent');
                    //console.log(pIndex);
                    var pDesc = $(this).attr('desc');
                    var pGroup = $(this).attr('group');

                    var items = $('input[id^=' + pGroup + pIndex + '_]');

                    $.each(items,
                        function () {
                            var type = $(this).attr('type');
                            var item = $(this);
                            var itemid = item.attr('id');
                            var itemAttribution = type === 'text' ? '' : $(this).prop('checked') ? 'checked' : '';
                            //var itemDesc = item.attr('desc');
                            var itemValue = item.val();
                            tableData.push({
                                ParentIndex: pIndex,
                                ParentDesc: pDesc,
                                ParentGroup: pGroup,

                                DataSign: itemid,
                                Attribution: itemAttribution,
                                //ItemDesc: itemDesc,
                                ItemValue: itemValue
                            });
                        });
                });

            var addtionItems = $('[desc="addtion"]');
            $.each(addtionItems,
                function () {
                    var type = $(this).attr('type');
                    var item = $(this);
                    var itemid = item.attr('id');
                    var itemAttribution = type === 'text' ? '' : $(this).prop('checked') ? 'checked' : '';
                    var itemValue = item.val();
                    var pDesc = item.attr("desc");
                    var pGroup = item.attr('group');
                    tableData.push({
                        ParentIndex: 0,
                        ParentDesc: pDesc,
                        ParentGroup: pGroup,

                        DataSign: itemid,
                        Attribution: itemAttribution,
                        //ItemDesc: itemDesc,
                        ItemValue: itemValue
                    });
                });

