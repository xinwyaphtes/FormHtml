                    var o = needcheckckbox(objs);

                    if (o) {
                        flag = true;
                        alert('请完善全部内容后点击下一页');
                    }
                    break;
                default:
            }

            return flag;
        }

        function needcheckckbox(objs) {
            var o = false;

            $.each(objs,
                function () {
                    if (!$('div[show = "true"]').find(this.group).is(':checked')) {
                        o = true;
                        return false;
                    } else {
                        $.each(this.aftersel,
                            function () {
                                var ischeck = this.substring(0, this.indexOf('|'));
                                var after = this.substring(this.indexOf('|') + 1);

                                if ($('div[show = "true"]').find(ischeck).is(':checked')) {
                                    $.each($('div[show = "true"]').find(after), function () {
                                        if ($(this).attr('type') == 'checkbox' && $(this).is(':checked')) {
                                            o = false;
                                            return false;
                                        }
                                        else if ($(this).attr('type') == 'checkbox' && !$(this).is(':checked')) {
                                            o = true;
                                        }
                                        else if ($(this).val() == '') {
                                            o = true;
                                            return false;
                                        }
                                    })
                                }
                            });
                    }
                });

            return o;
        }

        function needchecktext(obj) {
            var o;
            $.each(obj,
                function () {
                    if ($(this).val() == '') {
                        o = $(this);
                        return false;
                    }
                });
            return o;
        }

        function save() {
            var tableData = [];
            var childInfo = {
                Name: $('input[name="name"]').val(),
                Type: 1,
                Company: $('input[name="school"]').val(),
                Department: $('select[name="dept"]').val() + $('input[name="class"]').val(),
                EmployeeNo: $('input[name="stdno"]').val(),
                Birthday: $('input[name="birthday"]').val(),
            }

            $.each($('[parent]'),
                function () {
                    var pIndex = $(this).attr('parent');
                    //console.log(pIndex);
                    var pDesc = $(this).attr('desc');
                    var pGroup = $(this).attr('group');

                    var items = $('input[id^=' + pGroup + pIndex + '_]');

                    $.each(items,
                        function () {
                            var type = $(this).attr('type');
                            var item = $(this);
                            var itemid = item.attr('id');
                            var itemAttribution = type === 'text' ? '' : $(this).prop('checked') ? 'checked' : '';
                            //var itemDesc = item.attr('desc');
                            var itemValue = item.val();
                            tableData.push({
                                ParentIndex: pIndex,
                                ParentDesc: pDesc,
                                ParentGroup: pGroup,

                                DataSign: itemid,
                                Attribution: itemAttribution,
                                //ItemDesc: itemDesc,
                                ItemValue: itemValue
                            });
                        });
                });

            var addtionItems = $('[desc="addtion"]');
            $.each(addtionItems,
                function () {
                    var type = $(this).attr('type');
                    var item = $(this);
                    var itemid = item.attr('id');
                    var itemAttribution = type === 'text' ? '' : $(this).prop('checked') ? 'checked' : '';
                    var itemValue = item.val();
                    var pDesc = item.attr("desc");
                    var pGroup = item.attr('group');
                    tableData.push({
                        ParentIndex: 0,
                        ParentDesc: pDesc,
                        ParentGroup: pGroup,

                        DataSign: itemid,
                        Attribution: itemAttribution,
                        //ItemDesc: itemDesc,
                        ItemValue: itemValue
                    });
                });

            $.post('/FormData/SaveChildData',
                {
                    //visitID: visitID,
                    //name: $('#left_menu li.active a').html(),
                    //templateId: $('#left_menu li.active a').data('tid'),
                    //key: $('#hd_mainId').val(),
                    childInfo: childInfo,
                    data: JSON.stringify(tableData),
                    //dataResult: dataResult
                },
                function (data) {
                    $('#content').html("<h3>您已完成全部问卷，谢谢合作！</h3>");
                });

        }
    </script>
