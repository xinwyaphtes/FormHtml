                        alert('请完善全部内容后点击下一页');
                    }
                    break;
                case 3: break;
                    var objs = [{ group: '#A2_1,#A2_3,#A2_5,#A2_7,#A2_8', aftersel: ['#A2_1|#A2_2', '#A2_3|#A2_4', '#A2_5|#A2_6'] }]
                    var o = needcheckckbox(objs);

                    if (o) {
                        flag = true;
